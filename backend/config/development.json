{
  "http": {
    "port": 8888,
    "prefix": "/api",
    "origins": ["http://localhost:8080", "http://localhost:8081"],
    "throttle": {
      "burst": 20,
      "rate": 2,
      "ip": false,
      "xff": true
    }
  },
  "authentication": {},
  "logger": {
    "streams": {
      "console": {
        "level": "debug"
      }
    }
  },
  "redis": {
    "port": 6379,
    "host": "localhost",
    "database": 0,
    "reconnect_after": 2000
  },
  "sqlite": {
    "filename": "dapp.db"
  },
  "proxy": {
    "openPortTimeout": 3600,
    "checkPortInterval": 1000,
    "checkPortAttempts": 30,
    "baseUrl": "http://localhost/app/"
  }
}
