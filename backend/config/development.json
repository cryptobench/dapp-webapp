{
  "http": {
    "port": 8888,
    "prefix": "/api",
    "origins": ["http://localhost:8080", "http://localhost:8081"],
    "throttle": {
      "burst": 20,
      "rate": 2,
      "ip": false,
      "xff": true
    }
  },
  "cli": {
    "command": "dapp-manager",
    "args": [],
    "cwd": null,
    "env": {
      "OPTIONAL_SAMPLE_VAR": "var passed to env of spawned process - to define by user if needed"
    }
  },
  "authentication": {},
  "logger": {
    "streams": {
      "console": {
        "level": "debug"
      }
    }
  },
  "redis": {
      "port": 6379,
      "host": "localhost",
      "database": 0,
      "reconnect_after": 2000
  },
  "sqlite": {
    "filename": "dapp.db"
  }
}
